    "794aaeca-d775-460e-8117-89498e1d2694": {
      "type": "rich-text",
      "blocks": {
        "f47948dc-2fbc-4b7a-b2f0-d2430202c673": {
          "type": "heading",
          "settings": {
            "heading": "<p><strong>3 things to know <\/strong><em><strong>before <\/strong><\/em><strong>you buy any butterbur product<\/strong><\/p>",
            "heading_size": "h1"
          }
        }
      },
      "block_order": [
        "f47948dc-2fbc-4b7a-b2f0-d2430202c673"
      ],
